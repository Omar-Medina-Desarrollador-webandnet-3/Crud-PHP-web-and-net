//Creando un nuevo repositrio en la linea de comando

echo "# Crud-PHP-web-and-net" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/Omar-Medina-Desarrollador-webandnet-3/Crud-PHP-web-and-net.git
git push -u origin main


